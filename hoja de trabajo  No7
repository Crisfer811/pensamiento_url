print("ejercicio 1")
for i in range (1,11):
    if i % 2 > 0:
        print(i)
        
print("ejericio 2")
x = 1
while x < 11:
    if x % 2 != 0:
        print (x)
    x += 1
    
print("ESCENARIO 1")
while True:
    palabra = input("ingrese una palabra y si desea salir del bucle ingrese chupacabra: ")
    if palabra == "chupacabra":
        print ("has salido del bucle")
        break     
    
print("ESCENARIO 2")
palabra = input("ingrese una palabra: ")
palabra_compuesta = palabra.upper()
for comer in palabra_compuesta:
    if comer in "AEIOU":
        continue
    print(comer)
